{
    "compilerOptions":{
        "target": "es6",
        "baseUrl": "src",
        "jsx": "react",
        "paths": {
            "components:/*":[
                "./components/*"
            ],
            "containers:/*":[
                "./containers/*"
            ],
            "commons:/*":[
                "./commons/*"
            ]
            , "assets:/*":[
                "./assests/*"
            ]
            
        }
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}